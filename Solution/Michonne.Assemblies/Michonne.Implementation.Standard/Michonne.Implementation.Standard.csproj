  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Michonne.Interfaces.Standard\Michonne.Interfaces.Standard.csproj" />
  </ItemGroup>

  <Import Project="..\..\Michonne.Sources\Michone.Sources.projitems" Label="Shared" />

